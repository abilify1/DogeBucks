             let data = body.slice(15)
             let buff = new Buffer(data)
             let base64data = buff.toString('base64')
             client.reply(from, `‚û∏ *Text* : ${data}\n‚û∏ *Result* : ${base64data}`,id)
             break
            } else if (args[1] == 'decode') {
              let data = body.slice(15)
              let buff = new Buffer(data,"base64")
              let asciidata = buff.toString("ascii")
              client.reply(from,`‚û∏ *Base64* : ${data}\n‚û∏ *Result* : ${asciidata}`,id)
              break
            } else {
              client.reply(from, `Kirim perintah encode atau decode!!!`,id)
            }
            break
