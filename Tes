             if (nut.StatusMessage == 'Request misscall berhasil') return client.reply(from,`Spam call [NUTRICLUB] berhasil ke nomor *${args[2]}*`,id)
             else return client.reply(from,`Spam call [NUTRICLUB] gagal ke nomor *${args[2]}*`,id)
